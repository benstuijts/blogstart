<h2>You liked this articles</h2>

<ul class="list-group">
<% user.articles.liked.forEach(function(article){ %>
    
    <li class="list-group-item">
        <a href="<%= baseUrl %><%= article.slug %>"><%= article.title %></a>
        <a href="profile/article/dislike?id=<%= article.id %>" class="close" aria-label="Close"><span aria-hidden="true">&times;</span></a>
    </li>    
    
<% }) %>    
</ul>